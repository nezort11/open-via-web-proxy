{
  "manifest_version": 3,
  "version": "1.4",
  "name": "Open via Web Proxy",
  "description": "Open any website via online web proxy. Enjoy viewing any content without censorship restrictions without installing VPN services.",
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "webNavigation",
    "tabs"
  ],
  "action": {},
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "icons": {
    "16": "webproxy16.png",
    "32": "webproxy48.png",
    "48": "webproxy48.png",
    "128": "webproxy128.png"
  }
}
